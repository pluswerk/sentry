{
  "name": "pluswerk/sentry",
  "description": "+Pluswerk TYPO3 extension: Sentry Client",
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "homepage": "https://www.pluswerk.ag/",
  "support": {
    "issues": "https://github.com/pluswerk/sentry/issues",
    "source": "https://github.com/pluswerk/sentry",
    "docs": "https://github.com/pluswerk/sentry/blob/master/README.md"
  },
  "require": {
    "php": "~8.0.0 || ~8.1.0 || ~8.2.0",
    "ext-fileinfo": "*",
    "composer-runtime-api": "^2",
    "http-interop/http-factory-guzzle": "^1.0",
    "sentry/sentry": "^3.22.1",
    "typo3/cms-core": "^11.5 || ^12.4",
    "typo3/cms-frontend": "^11.5 || ^12.4"
  },
  "require-dev": {
    "phpstan/extension-installer": "^1.2",
    "pluswerk/grumphp-config": "^6",
    "saschaegerer/phpstan-typo3": "^1.8",
    "ssch/typo3-rector": "^1.1.3",
    "symfony/http-client": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "Pluswerk\\Sentry\\": "Classes/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "php-http/discovery": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "pluswerk/grumphp-config": true,
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    },
    "sort-packages": true
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "sentry"
    }
  }
}
